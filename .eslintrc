{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [1, "single"],
    "eqeqeq": 1,
    "dot-notation": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  },
  "plugins": [
    "react"
  ],
  "extends": "eslint:recommended"
}
